[user]
	name = Roman Perminov
	email = permidon@me.com
[core]
	autocrlf = false
	safecrlf = true
	excludesfile = ~/.gitignore
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[push]
	default = simple
